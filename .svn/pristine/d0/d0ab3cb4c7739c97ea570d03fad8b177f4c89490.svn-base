        loading: false,
        mallId: '',
        total: 0,
        preData:[],
        currentPage: 1, // 页码
        pageSize: 20, // 页码
        generateIndex:'',
        generateCount:'',
        mall_name: [], // 门店信息
        pickerOptions: {
          shortcuts: [{
            text: '最近一周',
            onClick(picker) {
              const end = new Date()
              const start = new Date()
              start.setTime(start.getTime())
              end.setTime(end.getTime() - 3600 * 1000 * 24 * 7)
              picker.$emit('pick', [end, start])
            }
          }, {
            text: '最近一个月',
            onClick(picker) {
              const end = new Date()
              const start = new Date()
              start.setTime(start.getTime())
              end.setTime(end.getTime() - 3600 * 1000 * 24 * 30)
              picker.$emit('pick', [end, start])
            }
          }, {
            text: '最近一年',
            onClick(picker) {
              const end = new Date()
              const start = new Date()
              start.setTime(start.getTime())
              end.setTime(start.getTime() - 3600 * 1000 * 24 * 365)
              picker.$emit('pick', [end, start])
            }
          }]
        },
        tableData: [], // 订单table
        agencyList: '', // 单行点击的数据
        start_date: '', // 起始日期
        start_account_date: '', // 立账起始日期
        searchRight: 'el-icon-arrow-down',
        formShow: false,
        statusClick: {
          inCheckings: true,//提交审核
          revokeInCheckings: true,//撤销审核中
          checkings: true,//审核
          cancle: true,//取消执行
