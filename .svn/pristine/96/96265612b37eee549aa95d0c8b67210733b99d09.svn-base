.el-form {
  margin: 10px;
  padding: 10px;
  background: #f8f8f8 !important;
}
.el-dialog__header{
  padding: 10px 20px;
}
.el-dialog__body{
  padding: 10px 20px;
}
.el-date-editor--daterange.el-input__inner {
  width: 100%;
}
.el-select .el-input__inner {   //select的右边距调整
  cursor: pointer;
  padding-right: 15px;
}
.el-cascader { //层级选择器width位100%
  width: 100%;
}
.el-input__inner {
  border-radius: 2px;
  border: 1px solid #e4e4e4;
}

.el-select {
  width: 100%;
}

.btn_bottom {
  margin-bottom: 5px;
}

.cell {
  button span {
    padding-right: 15px;
    border-right: 1px solid #6aa3a9;
  }

  button:last-child span {
    border-right: 0;
  }
}

.elem_quote {
  margin-bottom: 10px;
  padding: 10px;
  line-height: 22px;
  border-left: 5px solid #009688;
  border-radius: 0 2px 2px 0;
  background-color: #f2f2f2;
  color: #888d92;
  font-size: 14px;
}


.label_required>.el-form-item__label:before {
  content: "*";
  color: #ff0000;
}
.el-dialog__wrapper{
  background: rgba(0, 0, 0, 0.34);
}
