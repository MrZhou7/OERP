<script>
  import {common} from '@/common/common'
  export default {
    name: "checkBusiness",
    props:{
      checkData:{
        type:Object
      }
    },
    data(){
      return{
        customerRank:[],//商戶級別信息
        cityProps:{
          children: 'children',
          label: 'text',
          value: 'id'
        },
        cityData:[]//请求本地的城市文件
      }
    },
    methods:{
      //获取城市地点文件
      getCityData(){
        this.axios.post(process.env.BASE_CITY).then((res)=>{
          this.cityData = res.data;
        })
        common.lookup('L007', this).then((res) => {
          this.customerRank = res
        })
      }
    },
    created() {
      this.getCityData()
    }
  }
</script>

