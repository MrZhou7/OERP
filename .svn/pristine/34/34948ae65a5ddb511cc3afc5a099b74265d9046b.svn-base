<script>
  import {del} from '../../common/del'
export default {
  data() {
    return {
      formInline:{},
      tableData:[],
      type:'',
      fieldChoice: false,
      preData:{}
    };
  },
  components: {
  },
  created: function() {
    const that = this;
    this.axios
      .post(process.env.BASE_API + "contract_main/getcontractlist", {
        mall_id: 1
      })
      .then(function(res) {
        if (res.data.code == 0) {
          that.tableData = res.data.data;
        }
      })
      .catch(function(err) {
        that.$message.error(err.responseJSON.msg);
      });
    this.getPreData()
  },
  methods: {
    getPreData() {//预渲染参数
      let that = this;
      this.axios
        .post(process.env.BASE_API + "table_util/getPreData", {act: 'contract'})
        .then(function (res) {
          if (res.status == 200) {
            that.preData = res.data.data
          }
        })
        .catch(function (err) {
          this.$message.error(err.response.data.msg);
        });
    },
    onSecher() {

      const that = this;
      console.log(this.formInline)
      this.axios
        .post(process.env.BASE_API + "contract_main/getcontractlist", {
          mall_id: this.formInline.mall_id
        })
        .then(function(res) {
          if (res.data.code == 0) {
            that.tableData = res.data.data;
          }
        })
        .catch(function(err) {
        });
    },
    addRow() {
      this.$router.push({
        path: '/contract/create',
        query: {
          status: 'add'
        }
      })
    },
    viewRow(index, row) {
      this.$router.push({
        path: '/contract/create',
        meta: { title: '查看租赁合同' },
        query: {
          status: 'view',
          type: row[index]["contract_main_id"]
        }
      })
    },
    editRow(index, row) {
      this.$router.push({
        path: '/contract/create',
        meta: { title: '编辑租赁合同' },
        query: {
          status: 'edit',
          type: row[index]["contract_main_id"]
        }
      })
    },
    deleteRow(index, row) {
      del('合同',process.env.BASE_API + "contract_main/getcontractlist",{contract_main_id: row[index].contract_main_id}, this)
    },

  }
};
</script>

